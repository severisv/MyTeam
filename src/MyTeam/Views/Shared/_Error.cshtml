@using System.Threading.Tasks
@using MyTeam.Models.Enums
@using MyTeam.Resources
@model Exception
@{
    Layout = null;
    var guid = Guid.NewGuid();
}

<div mt-alert="@AlertType.Danger" >
    @Res.Error
</div>

@if (Model != null)
{
    <p>
        <a data-toggle="collapse" href="#stacktrace-@guid"><i class="fa fa-warning"></i> Stacktrace</a>
    </p>
        <div id="stacktrace-@guid" class="panel-collapse collapse">
            @Html.Partial("_ErrorSummary")
        </div>
}