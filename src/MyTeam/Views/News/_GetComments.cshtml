@using System.Threading.Tasks
@using MyTeam.ViewModels.News
@model GetCommentsViewModel
@foreach (var comment in Model.Comments)
{
    @Html.Partial("_GetComment", comment)
}

@if (Context.UserIsMember())
{
    @Html.Partial("_PostComment", new PostCommentViewModel
{
    ArticleId = Model.ArticleId
})
}
