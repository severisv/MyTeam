{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/*.bundle.*": true,
    "**/style.css": true,
    "**/.deploy": true,
    "**/obj": true,
    "**/.fake": true,
    "**/bin": true,
    "**/node_modules": true,
    "client/public": true,
    "**/bundle.*": true,
    "**/wwwroot/compiled/*": true,
    "**/*.fs.js": true,
    "**/*.map": true,
    "**/.paket": true,
    "**/paket-files": true,
    "dist": true,
    "artifacts": true,
  },
  "fantomas.noSpaceAfterComma": false,
  "fantomas.noSpaceAfterSemiColon": false,
  "fantomas.noSpaceAroundDelimiter": false,
  "fantomas.noSpaceBeforeColon": false,
  "fantomas.reorderOpenDeclaration": true,
  "FSharp.fsacRuntime": "netcore",
  "fantomas.pageWidth": 100
}